{
  "family": "demo-app",
  "containerDefinitions": [{
    "portMappings": [{
      "containerPort": 3000,
      "protocol": "tcp",
      "hostPort": 3000
    }],
    "name": "demo-app",
    "image": "<IMAGE1_NAME>",
    "essential": true
  }],
  "cpu": "256",
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::919139699099:role/ecsTaskRole",
  "executionRoleArn": "arn:aws:iam::919139699099:role/ecsTaskExecutionRole",
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "networkMode": "awsvpc"
}